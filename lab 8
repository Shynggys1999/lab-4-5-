def reduce(function, iterable, initializer=None):
    it = iter(iterable)
    if initializer is None:
        value = next(it)
    else:
        value = initializer
    for element in it:
        value = function(value, element)
    return value

    >>> from functools import reduce
>>> items = [10, 20, 30, 40, 50]
>>> sum_all = reduce(lambda x,y: x + y, items)
>>> sum_all
# 150